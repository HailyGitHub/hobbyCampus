<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 <mapper namespace="hobbycam.resume">
 	<insert id="insertResume" parameterType="com.hobbycam.resume.model.ResumeDTO">
 		INSERT INTO
 			teacher_resume
 			VALUES(
 				seq_teacher_resume.nextval,
 				1,
 				1,
 				#{resume_name},
 				1,
 				#{resume_intro},
 				#{resume_plan},
 				to_date(#{interview_date_param}, 'yyyy-mm-dd'),
 				'false',
 				'false',
 				#{interview_date_param}
 			)
 	</insert>
 	<insert id="dateTest" parameterType="test.TestDTO">
 		INSERT INTO
 			dateTest
 				VALUES(
 					#{name},
 					#{testdate1},
 					to_date(#{testdate1},'yyyy-mm-dd')
 				)
 	
 	</insert>
 </mapper>
